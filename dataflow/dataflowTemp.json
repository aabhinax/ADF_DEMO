{
	"name": "dataflowTemp",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceSalesDataTemp",
						"type": "DatasetReference"
					},
					"name": "sourceSales"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter"
				},
				{
					"name": "aggregate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as string,",
				"          QUANTITYORDERED as string,",
				"          PRICEEACH as string,",
				"          ORDERLINENUMBER as string,",
				"          SALES as integer,",
				"          ORDERDATE as string,",
				"          STATUS as string,",
				"          QTR_ID as string,",
				"          MONTH_ID as string,",
				"          YEAR_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> sourceSales",
				"sourceSales filter(SALES>='2000') ~> filter",
				"filter aggregate(groupBy(YEAR_ID,",
				"          MONTH_ID),",
				"     TOTALSALES = sum(SALES)) ~> aggregate",
				"aggregate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          YEAR_ID as string,",
				"          MONTH_ID as string,",
				"          TOTALSALES as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true) ~> sink1"
			]
		}
	}
}